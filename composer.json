{
    "name": "harikt/web.expressive",
    "description": "The integration package with the zend expressive",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Elliot Levin",
            "email": "elliotlevin@hotmail.com"
        },
        {
            "name": "Hari KT",
            "email": "kthari85@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Dms\\Web\\Expressive\\": "./src/"
        },
        "files" : [
            "./src/helper.php"
        ]
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "harikt/blade-renderer",
                "version": "0.1.0",
                "source": {
                    "type": "git",
                    "url": "https://github.com/harikt/blade-renderer",
                    "reference": "master"
                },
                "autoload": {
                    "psr-4": {
                        "Harikt\\Blade\\": "src/"
                    }
                },
                "autoload-dev": {
                    "psr-4": {
                        "Harikt\\Blade\\": "tests/"
                    }
                },
                "require": {
                    "illuminate/container": "^5.5",
                    "illuminate/events": "^5.5",
                    "illuminate/filesystem": "^5.5",
                    "illuminate/view": "^5.5",
                    "psr/container": "~1.0",
                    "zendframework/zend-expressive-template": "^1.0"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "aura/session": "^2.1",
        "cache/cache": "^0.1.0",
        "dms-org/core": "^0.8",
        "dms-org/common.structure": "^0.8",
        "harikt/blade-renderer": "0.1.0",
        "illuminate/cache": "^5.4",
        "illuminate/config": "^5.4",
        "marcj/topsort": "^1.0",
        "league/oauth2-google": "^1.0",
        "laravel/browser-kit-testing": "^1.0",
        "symfony/translation": "^3.3",
        "zendframework/zend-expressive": "^2.0.2"
    },
    "autoload-dev": {
        "psr-4": {
            "Dms\\Web\\Expressive\\Tests\\": "./tests/Tests/",
            "Dms\\Core\\Tests\\": "./vendor/dms-org/core/tests/Tests/"
        }
    },
    "require-dev": {
        "dms-org/common.testing": "dev-master",
        "orchestra/testbench": "~3.0",
        "dms-org/package.content": "^0.8"
    }
}
